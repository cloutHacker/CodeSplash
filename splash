<?php
//including  all the classes used

require __DIR__.'/vendor/autoload.php';

/*
 |start the app kernel for commandline
*/

use App\Console\Kernel;

/*
|Running the console for validation and execution of terminal commands
|Assigning all the commands to the right fuctions
*/

(new Kernel())->validateCommand($argv)->assignCommand();

